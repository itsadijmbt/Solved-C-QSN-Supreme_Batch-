QSN : Finds unique elemt while others are in pairs 

Sol:#include <iostream>
using namespace std;

int main() {
   
     int n,ans=0;
     cin>>n;
     
     int a[n];
     
     
     for(int i=0;i<n;i++){cout<<"enter"<<"\n"; cin>>a[i];}
     
     for(int i=0;i<n;++i) ans=ans^a[i];
     

     
    cout<<ans<<"\n";
    return 0;
}
