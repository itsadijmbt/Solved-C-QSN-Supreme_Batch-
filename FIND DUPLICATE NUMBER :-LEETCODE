QSN:https://leetcode.com/problems/find-the-duplicate-number/description/

SOLN:
class Solution {
public:
    int findDuplicate(vector<int>& nums) {
         int n=nums.size();
         int a[n];
         memset(a, 0, (n)* sizeof(int));
         int ans=0;
  
        for(int i=0;i<n;++i)a[(nums[i]-1)]++;
           
        for(int i=0;i<n;++i)if(a[i]>=2)ans=i+1;
               
        return ans;
    }
};
